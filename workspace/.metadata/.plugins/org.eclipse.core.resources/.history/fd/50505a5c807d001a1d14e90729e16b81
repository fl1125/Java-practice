package com.topractice;

import java.util.Scanner;

/*
 *   键盘录入5个int类型的数据存储数组arr中
 *   定义方法将arr数组中的内容反转
 *   定义方法对反转后的数组进行遍历
 * 
 * 
 * 
 * 
 * 
 */
public class Test6 {

	public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int[] arr= new int[5];
        
        System.out.println("请输入五个数据");
        for(int x=0; x<arr.length; x++){
        	int a = sc.nextInt();
        	a = arr[x];
        } 
	}

}
