package com.itpractice;
/*
 * 学生类(带有参数的构造方法)
 * 
 * 
 */
public class Student {
 
	private String name;
	private int age;
	
	public void student(){
	}
	
	public void Student(String name,int age){
		this.name = name;
		this.age = age;
	}
	
	public void show(){
		System.out.println(name+"----"+age);
	}

}
