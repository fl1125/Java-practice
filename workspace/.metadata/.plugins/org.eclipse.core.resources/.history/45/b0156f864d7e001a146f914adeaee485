package com.itpracice9;

public class StudentDemo {

	public static void main(String[] args) {
//		 带参构造
	Student s = new Student();
	 s.Student("jack", 18);
	 s.show();
	 
//	 无参+setXxx()
	 Student s2 = new Student();
	 s.setAge(18);
	 s.setName("jack");
	 System.out.println(s.geiName+s.getAge);
	}

}
