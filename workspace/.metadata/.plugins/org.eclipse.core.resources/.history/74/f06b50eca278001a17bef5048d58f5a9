package com.itpractice;
// 键盘录入三个数据，并获取这三个数据中的最大值

import java.util.Scanner;

public class ScannerDemo3 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("请输入第一个数据：");
		int a = sc.nextInt();
		System.out.println("请输入第一个数据：");
		int b = sc.nextInt();	
		System.out.println("请输入第一个数据：");
		int c = sc.nextInt();

	}

}
