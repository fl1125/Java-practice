package com.itpractice4;

/* 输出所有的水仙花数
       水仙花数：就是一个三位数，其各位数字的立方和是该数字的本身。
       举例：153  153=1*1*1+5*5*5+3*3*3
 
 
*/

public class ForTest4 {

	public static void main(String[] args) {
         
		for(int x=100; x<1000; x++){
			
			int ge = x%10;
			int shi = x/10%10;
			int bai = x/10/10%10;
			
			if((ge*ge*ge*+shi*shi+shi*shi*shi)==x)
			{
				System.out.println(x);
			}
		}
		System.out.println("he");
	}
}
