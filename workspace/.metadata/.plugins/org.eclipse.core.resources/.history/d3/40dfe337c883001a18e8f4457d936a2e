package com.itpractice;
/*
 * 继承中成员方法的特点
 * 
 */
public class ExtendsDemo4 {

	public static void main(String[] args) {
	  Kid1 k1 = new Kid1();
	  k1.eat();

	}
}
  class Dad1{
	  public void eat(){
		  System.out.println("喝酒");
		  System.out.println("睡觉"); 
	  }
  }
   class Kid1 extends Dad1{
	   public void eat(){
		   System.out.println("好好吃饭");
	   }
   }