package com.itpractice2;
/*    if语句格式2：
 *       if(关系表达式){
 *       语句体1;}
 *       else{
 *       语句体2;
 *       }
 * 
 * 
 * 
 * 
 */
public class IfDemo2 {

	public static void main(String[] args) {
		int a = 10;
		int b = 20;
		
		if(a==b){
			System.out.println("a等于b");
		}
		else{
			System.out.println("a不等于b");
		}

	}

}
