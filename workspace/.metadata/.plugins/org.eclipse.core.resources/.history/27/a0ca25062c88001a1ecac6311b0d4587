package com.itpractice;

public class RecurrenceDemo2 {

	public static void main(String[] args) {
		int result = method(8);
		System.out.println(result);
		
	}
	
	public static int method(int n){
		
		if(n==1){
			return 1;
		}else if(n == 2){
			return 1;
		}
		else{
			return method(n -1) + method(n -2);
		}
	}

}
