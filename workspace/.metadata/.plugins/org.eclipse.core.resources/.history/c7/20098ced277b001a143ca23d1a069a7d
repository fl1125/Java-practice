package com.itpractice;

import java.util.Scanner;

//键盘录入两个数据，返回两个数中的较大值

public class MethodTest {

	public static void main(String[] args){
       Scanner sc = new Scanner(System.in);
       int a = sc.nextInt();
       int b = sc.nextInt();
		int result = max
	}
    
	
	public static int max(int a,int b){
		int c = 0;
		if(a>b){
			c = a;
		}else{
			c = b;
		}
		return c;
		
	}
}
