package com.itpracice9;
/*
 * 学生类(带有参数的构造方法)
 * 
 * 
 */
public class Student {
 
	private String name;
	private int age;
	
	public void student(){
	}
//	构造方法
	public void Student(String name,int age){
		this.name = name;
		this.age = age;
	}
//	成员方法
	public void setName(String name){
		this.name = name;
	} 
	public void geiName(String name){
		return name;
	}
	public void show(){
		System.out.println(name+"----"+age);
	}

}
