	public static void main(String[] args) {
          
		ArrayList<String> array = new ArrayList<String>();
		
		array.add("张三丰");
		array.add("宋远桥");
		array.add("张无忌");
		array.add("殷梨亭");
		array.add("张翠山");
		array.add("莫声谷");
		
		for(int x=0; x<array.size(); x++){
			
		}
		
	}

}
