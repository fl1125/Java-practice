package com.itpractice4;
/*
 * 逻辑运算符：用于连接关系表达式
 *  &，|，^,!
 *  &&,||
 *  
 *  
 * 
 * 
 * 
 * 
 * 
 * 
 */
public class OpeatorDemo {

	public static void main(String[] args) {
	    int a = 3;
	    int b = 4;
	    int c = 5;
//	&
	    System.out.println((a>b) & (a<c));
	}

}
