package itpractice;

public class ArrayTest1 {

	public static void main(String[] args) {
       int[] arr = new int[3];
       
       System.out.println(arr);
       System.out.println(arr[0]);
       System.out.println(arr[1]);
       System.out.println(arr[2]);
//       И│ох
       arr[0] = 100;
       arr[2] = 200;
	}

}
