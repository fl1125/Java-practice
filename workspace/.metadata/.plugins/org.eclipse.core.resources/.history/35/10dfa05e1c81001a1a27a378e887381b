package com.itpractice;

import java.util.ArrayList;

/*
 * 
 * ArratList集合的遍历
 * 
 */
public class ArraListDemo3 {

	public static void main(String[] args) {
		ArrayList<String> array = new ArrayList<String>();
		
		array.add("hello");
		array.add("world");
		array.add("java");
		
//		获取元素
		for(int x=0;x<3;x++){
		System.out.println(array.get(x));
		}
		

	}

}
