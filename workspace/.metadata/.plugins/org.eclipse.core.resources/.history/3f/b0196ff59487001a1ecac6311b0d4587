package com.itpractice;
/*
 * Throwable的常用方法：
		String getMessage()  
		String toString()  
		void printStackTrace()  
 	
 * 	
 */
public class ExceptionDemo4 {

	public static void main(String[] args) {
		try{
		   System.out.println(2/0);
		}catch (ArithmeticException e) {
		}
		
	
		System.out.println(e.getMessage());

	}

}
