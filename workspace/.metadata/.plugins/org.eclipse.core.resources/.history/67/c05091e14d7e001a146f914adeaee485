package com.itpracice9;

public class StudentDemo {

	public static void main(String[] args) {
//		 带参构造
	Student s = new Student();
	 s.Student("jack", 18);
	 s.show();
	 
//	 无参+setXxx()
	 Student s2 = new Student();
	 s2.setAge(18);
	 s2.getName("jack");
	 System.out.println(s2.getName()+s2.getAge());
	}

}
