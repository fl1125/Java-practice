package com.itpractice2;

import java.util.HashMap;

/*
 * 
 * 使用HashMap存储数据并遍历（字符串作为key）
 * 
 */
public class HashMapDemo {

	public static void main(String[] args) {
		//创建Map对象
		HashMap<String,String> hm = new HashMap<String,String>();
		//添加映射关系
		hm.put("ITCAST001", "张三");
		hm.put("ITCAST002", "李四");
		hm.put("ITCAST003", "王五");
		hm.put("ITCAST003", "赵六");
		//遍历Map对象
		
	}

}
