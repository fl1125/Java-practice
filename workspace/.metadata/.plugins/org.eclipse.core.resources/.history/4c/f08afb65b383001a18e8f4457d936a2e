package com.itpractice;
/*
 * 继承中成员变量的特点
 *     子类只能获取父类非私有成员
 */
public class ExtendsDemo3 {

	public static void main(String[] args) {
		Kid k = new Kid();
		k.show();
	}

}

class Dad{
	 String name = "jack";
}
class Kid extends Dad {
	String name = "black";
	public void show(){
		System.out.println(name);
	}
}