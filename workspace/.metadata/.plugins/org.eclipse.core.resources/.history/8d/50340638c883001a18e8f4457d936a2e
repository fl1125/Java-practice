package com.itpractice;
/*
 * 方法重写的用意场景：
 * 方法重写的注意事项：
 * 
 * 
 */
public class ExtendsDemo5 {

	public static void main(String[] args) {
	   NewPhone np = new NewPhone();
	   np.call();
	}

}
class Phone{
	public void call(){
		System.out.println("打电话");
	}
}
class NewPhone extends Phone{
	public void call(){
		System.out.println("录音");
		
		super.call();
	}
}